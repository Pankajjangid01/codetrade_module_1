<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="invoice_data" name="Invoice">
        <t t-call="web.html_container" style="border:1px solid black">

            <div style="width: 90%; height: 100%; position:relative;margin:auto;padding:25px;background-image: url(static/src/img/img1.png);">
                <div style="display: flex; align-items: center; justify-content: space-between;margin-top:-100px">
                    <div style="border-bottom: 100px solid #D3D3D3; border-right: 50px solid transparent; height: 100px; width: 420px;"></div>
                    <p style="color: white; font-size: 50px; padding: 10px;margin-top:-60px;margin-left:20px;  font-family:serif; font-weight: 400;font-style: normal;">
                        QUOTATION
                    </p>
                </div>
                <span>
                    <img t-att-src="'data:image/png;base64,'" alt="logo" style="position:absolute;right:10%;top:15px;height:10%;width:20%;" />
                </span>
                <div style="border-top: 150px solid #FFA500; border-right: 50px solid transparent; width: 360px; padding: 5px; margin-top: -70px;">
                    <div style="margin-top:-130px;margin-left:45px">
                        <div style="font-weight:bold;color:black;">Quotation to:</div>
                        <t t-foreach="docs" t-as="record">
                            <div style="font-weight:bold;color:black;"><span t-esc="record.partner_id.name"/></div>
                            <div><span t-esc="record.partner_id.street"/></div>
                            <div><span t-esc="record.partner_id.city"/>, <span t-esc="record.partner_id.zip"/></div>
                            <div><span t-esc="record.partner_id.country_id.name"/></div>
                            <div><span t-esc="record.partner_id.phone"/></div>
                        </t>
                    </div>
                </div>
                <div style="margin-right:10px;position:absolute;right:75px;top:160px;">
                    <t t-foreach="docs" t-as="record">
                        <div style="width:200px;">Date:<span t-esc="record.date_order"/></div>
                        <div style="margin-top:5px">Validity:<span t-esc="record.validity_date"/></div>
                        <div style="margin-top:5px">Quotation No:<span t-esc="record.name"/></div>
                    </t>
                </div>
                <table class="table table-bordered" style="margin-top: 20px; border-collapse: collapse; width: 100%;">
                    <thead style="border:1.5px solid #ddd;">
                        <tr style="text-align: left;border:1.5px solid #ddd;">
                            <th style="padding: 5px;border:1.5px solid #ddd;">Product Description</th>
                            <th style="padding: 5px;border:1.5px solid #ddd;">Quantity</th>
                            <th style="padding: 5px;border:1.5px solid #ddd;">Unit Price</th>
                            <th style="padding: 5px;border:1.5px solid #ddd;">Total</th>
                        </tr>
                    </thead>
                    <tbody style="border:1.5px solid #ddd;">
                        <t t-foreach="record.order_line" t-as="data">
                            <tr style="border:1.5px solid #ddd;">
                                <td style="padding: 10px;border:1.5px solid #ddd;"><span t-esc="data.name"/></td>
                                <td style="padding: 10px;border:1.5px solid #ddd;"><span t-esc="data.product_uom_qty"/></td>
                                <td style="padding: 10px;border:1.5px solid #ddd;"><span t-esc="data.price_unit"/></td>
                                <td style="padding: 10px;border:1.5px solid #ddd;">$<span t-esc="data.price_subtotal"/></td>
                            </tr>
                        </t>
                    </tbody>
                </table>
                <!-- Total and Tax -->
                <div style="margin-top: 10px; border-top: 2px solid black;width:15%; position:absolute; right:20px;">
                    <div style="margin-left:0px;margin-top:5px"><strong>Sub Total:</strong> <span t-esc="record.amount_untaxed"/></div>
                    <div style="margin-left:0px;margin-top:6px"><strong>Tax:</strong> <span t-esc="record.amount_tax"/></div>
                    <div style="border-top: 2px solid black;width:100%"/>
                    <div style="margin-left:0px;margin-top:5px"><strong>Total:</strong> <span t-esc="record.amount_total"/></div>
                </div>

                <div style="margin-top:45px;margin-left:10px">
                    <span style="color:727272;font-size:20px">Thank you for your business</span>
                </div>
                <!-- Payment info -->
                <div style="margin-left:10px; margin-top:15px;">
                    <span style="font-weight:400;font-size:20px">Payment Info:</span>
                    <div style="margin-top:5px"><span style="font-weight:bold;">Account No.:</span><span>03220200002128</span></div>
                    <div style="margin-top:5px"><span style="font-weight:bold;">Ac Name:</span></div>
                    <div style="margin-top:5px"><span style="font-weight:bold;">Bank Name:</span><span>Bank of Baroda</span></div>
                    <div style="margin-top:5px"><span style="font-weight:bold;">BankDetails:</span><span>NEFT/RTGS BARBOBHADRA</span></div>
                </div>
                <!-- Terms & Condition -->
                <div style="margin-left:10px; margin-top:30px">
                    <span style="font-weight:400;font-size:20px">Terms and Conditions</span>
                    <div style="margin-top:8px">Prices are ex.Godown Ahmedabad and all other charges are extra</div>
                    <div style="margin-top:8px">Frieght Insurance transportation extra at actual.</div>
                    <div style="margin-top:8px">100% Advance along with the Order.Warranty One Year against all Manufacturing Defects.</div>
                    <div style="margin-top:8px">Order confirmed can not be canceled.</div>
                </div>
                <!-- your company -->
                <div style="margin-left:10px; margin-top:40px">
                    <span style="font-weight:400;font-size:20px">YourCompany</span>
                    <div style="margin-top:6px">250 Executive Park Blvd, Suite 3400 , 94134 , San Francisco, United States</div>
                </div>
                <!-- watermark -->
                <div style="z-index: -1;">
                    <span style="-webkit-transform: rotate(-40deg); color:rgba(121, 114, 114, 0.83); font-size: 8em;position: absolute; opacity: 0.2;top:48%;left:32%">
                        M-kart
                    </span>
                </div>

               <div class="footer">
                    <div style="border-top: 1px solid #ddd;width:100%;margin-top:32px"/>
                    <div style="border-top: 60px solid #D3D3D3; border-right: 50px solid transparent;width:450px;margin-top:22px">
                        <div style="margin-top:-50px;margin-left:10px">+1 (650) 555-0111 , info@yourcompany.com</div>
                    </div>
                    <div style="border-bottom: 80px solid #FFA500; border-left: 60px solid transparent;width:450px;position:absolute;right:0px;margin-top:-80px"/>
                    <div style="margin-top:-50px; position:absolute; right:25%;"><a href="http://www.google.com" style="color:black; text-decoration: none;">http://google.com</a></div>
               </div>
            </div>
        </t>
    </template>
</odoo>
